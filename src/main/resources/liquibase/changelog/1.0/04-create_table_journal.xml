<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="26.01.2022-1" author="vmurzakov">
        <createTable schemaName="goodboys" tableName="journal">
            <column name="id" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_journal"/>
            </column>
            <column name="user_telegram_id" type="BIGINT" remarks="id пользователя в telegram">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="create_date" type="DATE" remarks="Дата когда был пушком"/>
        </createTable>
    </changeSet>

</databaseChangeLog>